<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="product_category_cyclos" model="product.category">
            <field name="parent_id" ref="lcc_lokavaluto_app_connection.product_category_numeric_lcc" />
            <field name="name">Cyclos</field>
        </record>
        <record id="product_product_cyclos" model="product.product">
            <field name="list_price">1.0</field>
            <field name="standard_price">1.0</field>
            <field name="uom_id" ref="uom.product_uom_unit" />
            <field name="uom_po_id" ref="uom.product_uom_unit" />
            <field name="name">Numeric LCC (Cyclos)</field>
            <field name="description_sale" eval="False" />
            <field name="categ_id" ref="lcc_cyclos_base.product_category_cyclos" />
            <field name="type">consu</field>
        </record>
    </data>
    <data>
        <record id="res_partner_cyclos_debit_wallet" model="res.partner">
            <field name="name">Cyclos Debit Wallet</field>
        </record>
        <record id="res_partner_backend_cyclos_debit_wallet" model="res.partner.backend">
            <field name="name">cyclos:system</field>
            <field name="type">cyclos</field>
            <field name="partner_id" ref="lcc_cyclos_base.res_partner_cyclos_debit_wallet" />
            <field name="cyclos_status">active</field>
            <field name="cyclos_id">system</field>
        </record>

    </data>
</odoo>