<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.module.category" id="module_category_exchange_counters">
        <field name="name">Exchange counters</field>
        <field name="description">Helps you get the most out of your exchange counters with fast exchange encoding, simplified payment mode encoding, automatic picking lists generation and more.</field>
        <field name="sequence">21</field>
    </record>

    <record id="group_exchange_counters_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_category_exchange_counters"/>
    </record>
    <record id="group_exchange_counters_manager" model="res.groups">
        <field name="name">Administrator</field>
        <field name="category_id" ref="module_category_exchange_counters"/>
        <field name="implied_ids" eval="[(4, ref('group_exchange_counters_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
</odoo>